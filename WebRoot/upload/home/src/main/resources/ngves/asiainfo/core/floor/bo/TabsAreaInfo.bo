<?xml version="1.0" encoding="GB2312"?>
<sysbolist>
    <sysbo datasource="UserDataSource" name="TabsAreaInfo">
        <mapingenty type="table">TABS_AREA_INFO</mapingenty>
        <datafilter />
        <attrlist>
            <attr floatlength="0" name="TABS_AREA_ID" maxlength="15" datatype="Long" type="PK" remark="区域标识">
                <mapingcol datatype="NUMBER">TABS_AREA_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="TABS_ID" maxlength="15" datatype="Long" type="COL" remark="页签标识">
                <mapingcol datatype="NUMBER">TABS_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="AREA_SORT" maxlength="20" datatype="String" type="COL" remark="区域排序">
                <mapingcol datatype="VARCHAR2">AREA_SORT</mapingcol>
            </attr>
            <attr floatlength="0" name="AREA_CONTENT_TYPE" maxlength="15" datatype="Long" type="COL" remark="区域数据类型
0：中类
1：小类
2：专题
3：品牌
4：活动">
                <mapingcol datatype="NUMBER">AREA_CONTENT_TYPE</mapingcol>
            </attr>
            <attr floatlength="0" name="AREA_CONTENT" maxlength="200" datatype="String" type="COL" remark="区域绑定内容">
                <mapingcol datatype="VARCHAR2">AREA_CONTENT</mapingcol>
            </attr>
            <attr floatlength="0" name="AREA_IMG_URL" maxlength="200" datatype="String" type="COL" remark="区域图片">
                <mapingcol datatype="VARCHAR2">AREA_IMG_URL</mapingcol>
            </attr>
        </attrlist>
        <relationlist />
        <oplist />
    </sysbo>
</sysbolist>
