<?xml version="1.0" encoding="UTF-8"?>
<sysbolist>
    <sysbo datasource="UserDataSource" name="UserBookInfo" mainattr="USER_BOOK_ID">
        <mapingenty type="table">USER_BOOK_INFO</mapingenty>
        <datafilter />
        <attrlist>
            <attr floatlength="0" name="USER_BOOK_ID" maxlength="15" datatype="Long" type="PK">
                <mapingcol datatype="NUMBER">USER_BOOK_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="INFO_CODE" maxlength="100" datatype="String" type="COL" remark="INFORMATION_KIND_INFO 表的流水，目前可以订阅的信息只有4种：邮件类型的兑换成功提醒、营销活动；短信类型的新品到货、到货提醒。">
                <mapingcol datatype="VARCHAR2">INFO_CODE</mapingcol>
            </attr>
            <attr floatlength="0" name="MOBILE_PHONE_NO" maxlength="15" datatype="String" type="COL" remark="邮件类型：01   短信类型：02">
                <mapingcol datatype="VARCHAR2">MOBILE_PHONE_NO</mapingcol>
            </attr>
            <attr floatlength="0" name="BOOK_STATUS" maxlength="2" datatype="String" type="COL" remark="00-未订阅  01-已订阅">
                <mapingcol datatype="VARCHAR2">BOOK_STATUS</mapingcol>
            </attr>
            <attr floatlength="0" name="SEND_DATE" maxlength="2" datatype="String" type="COL" remark="订阅短信类型的信息时，该字段值生效；周一至周五，周六日及公休日，不限制。">
                <mapingcol datatype="VARCHAR2">SEND_DATE</mapingcol>
            </attr>
            <attr floatlength="0" name="REBOOK_TIME" maxlength="20" datatype="DateTime" type="COL">
                <mapingcol datatype="DATE">REBOOK_TIME</mapingcol>
            </attr>
            <attr floatlength="0" name="BOOK_TIME" maxlength="20" datatype="DateTime" type="COL">
                <mapingcol datatype="DATE">BOOK_TIME</mapingcol>
            </attr>
            <attr floatlength="0" name="BAK_COL1" maxlength="15" datatype="Long" type="COL">
                <mapingcol datatype="NUMBER">BAK_COL1</mapingcol>
            </attr>
            <attr floatlength="0" name="BAK_COL2" maxlength="100" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">BAK_COL2</mapingcol>
            </attr>
            <attr floatlength="0" name="OPER" maxlength="20" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">OPER</mapingcol>
            </attr>
            <attr floatlength="0" name="USER_ID" maxlength="20" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">USER_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="USER_ROLE" maxlength="20" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">USER_ROLE</mapingcol>
            </attr>
        </attrlist>
        <relationlist />
        <oplist />
    </sysbo>
</sysbolist>
