<?xml version="1.0" encoding="UTF-8"?>
<sysbolist>
    <sysbo datasource="UserDataSource" name="WareKindInfo" mainattr="KIND_ID">
        <mapingenty type="table">WARE_KIND_INFO</mapingenty>
        <datafilter />
        <attrlist>
            <attr floatlength="0" name="KIND_ID" maxlength="15" datatype="Long" type="PK">
                <mapingcol datatype="NUMBER">KIND_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="KIND_CODE" maxlength="6" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">KIND_CODE</mapingcol>
            </attr>
            <attr floatlength="0" name="KIND_NAME" maxlength="400" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">KIND_NAME</mapingcol>
            </attr>
            <attr floatlength="0" name="KIND_STATE" maxlength="1" datatype="String" type="COL" remark="1表示有效,0表示无效,默认值是0">
                <mapingcol datatype="VARCHAR2">KIND_STATE</mapingcol>
            </attr>
            <attr floatlength="0" name="KIND_LEVEL" maxlength="2" datatype="Long" type="COL" remark="从第1级开始，递增,值必须大于0">
                <mapingcol datatype="NUMBER">KIND_LEVEL</mapingcol>
            </attr>
            <attr floatlength="0" name="SORT_ID" maxlength="5" datatype="Long" type="COL" remark="默认值是0">
                <mapingcol datatype="NUMBER">SORT_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="IS_NAVIGATE_VIEW" maxlength="1" datatype="String" type="COL" remark="默认是显示，导航按照排序号码进行排列,1表示显示,0表示不显示,默认是1">
                <mapingcol datatype="VARCHAR2">IS_NAVIGATE_VIEW</mapingcol>
            </attr>
            <attr floatlength="0" name="KIND_PARENT_ID" maxlength="15" datatype="Long" type="COL" remark="如果是-1标识自己就是第1级分类了">
                <mapingcol datatype="NUMBER">KIND_PARENT_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="KIND_PREFIX" maxlength="300" datatype="String" type="COL" remark="暂时可以为空,以后不能为空">
                <mapingcol datatype="VARCHAR2">KIND_PREFIX</mapingcol>
            </attr>
            <attr floatlength="0" name="TEMPLATE_ID" maxlength="15" datatype="Long" type="COL" remark="扩展以后分类之间展示方式不一样">
                <mapingcol datatype="NUMBER">TEMPLATE_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="VIEW_TEMPLATE" maxlength="50" datatype="String" type="COL" remark="扩展以后分类之间展示方式不一样">
                <mapingcol datatype="VARCHAR2">VIEW_TEMPLATE</mapingcol>
            </attr>
            <attr floatlength="0" name="BAK_COL" maxlength="50" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">BAK_COL</mapingcol>
            </attr>
            <attr floatlength="0" name="BAK_COL2" maxlength="50" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">BAK_COL2</mapingcol>
            </attr>
            <attr floatlength="0" name="BAK_COL3" maxlength="50" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">BAK_COL3</mapingcol>
            </attr>
            <attr floatlength="0" name="BAK_COL4" maxlength="50" datatype="String" type="COL" remark="是否第三方大类。1表示是，0表示不是">
                <mapingcol datatype="VARCHAR2">BAK_COL4</mapingcol>
            </attr>
            <attr floatlength="0" name="SHOW_CHANNEL_BIG" maxlength="20" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">SHOW_CHANNEL_BIG</mapingcol>
            </attr>
        </attrlist>
        <relationlist />
        <oplist />
    </sysbo>
</sysbolist>
