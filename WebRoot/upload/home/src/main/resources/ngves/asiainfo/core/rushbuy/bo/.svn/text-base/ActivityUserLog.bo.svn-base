<?xml version="1.0" encoding="UTF-8"?>
<sysbolist>
    <sysbo datasource="UserDataSource" name="ActivityUserLog" mainattr="ACTIVITY_AWARD_USER_ID">
        <mapingenty type="table">ACTIVITY_USER_LOG</mapingenty>
        <datafilter />
        <attrlist>
            <attr floatlength="0" name="ACTIVITY_AWARD_USER_ID" maxlength="15" datatype="Long" type="PK" remark="活动抢中用户唯一标识">
                <mapingcol datatype="NUMBER">ACTIVITY_AWARD_USER_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="ACTIVITY_ID" maxlength="15" datatype="Long" type="COL" remark="活动标识">
                <mapingcol datatype="NUMBER">ACTIVITY_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="WARE_ID" maxlength="15" datatype="Long" type="COL" remark="礼品标识">
                <mapingcol datatype="NUMBER">WARE_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="AWARD_STATE" maxlength="2" datatype="String" type="COL" remark="用户抢兑状态(
0:抢中
1:兑换
2:撤销
)">
                <mapingcol datatype="VARCHAR2">AWARD_STATE</mapingcol>
            </attr>
            <attr floatlength="0" name="AWARD_TIME" maxlength="20" datatype="DateTime" type="COL" remark="抢中时间">
                <mapingcol datatype="DATE">AWARD_TIME</mapingcol>
            </attr>
            <attr floatlength="0" name="EXCHANGE_TIME" maxlength="20" datatype="DateTime" type="COL" remark="兑换时间">
                <mapingcol datatype="DATE">EXCHANGE_TIME</mapingcol>
            </attr>
            <attr floatlength="0" name="CANCEL_TIME" maxlength="20" datatype="DateTime" type="COL" remark="撤销时间">
                <mapingcol datatype="DATE">CANCEL_TIME</mapingcol>
            </attr>
            <attr floatlength="0" name="ORDER_ID" maxlength="20" datatype="String" type="COL" remark="订单号">
                <mapingcol datatype="VARCHAR2">ORDER_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="MOBILE_NUMBER" maxlength="15" datatype="String" type="COL" remark="客户手机号">
                <mapingcol datatype="VARCHAR2">MOBILE_NUMBER</mapingcol>
            </attr>
            <attr floatlength="0" name="PROVINCE_CODE" maxlength="3" datatype="String" type="COL" remark="客户归属省代码">
                <mapingcol datatype="VARCHAR2">PROVINCE_CODE</mapingcol>
            </attr>
            <attr floatlength="0" name="CUSTOMER_BRAND" maxlength="4" datatype="String" type="COL" remark="客户星级">
                <mapingcol datatype="VARCHAR2">CUSTOMER_BRAND</mapingcol>
            </attr>
            <attr floatlength="0" name="USER_PAY_TYPE" maxlength="8" datatype="String" type="COL" remark="用户支付方式">
                <mapingcol datatype="VARCHAR2">USER_PAY_TYPE</mapingcol>
            </attr>
        </attrlist>
        <relationlist />
        <oplist />
    </sysbo>
</sysbolist>
