<?xml version="1.0" encoding="UTF-8"?>
<sysbolist>
    <sysbo datasource="UserDataSource" name="SmsOccasionInfo" mainattr="OCCASION_ID">
        <mapingenty type="table">SMS_OCCASION_INFO</mapingenty>
        <datafilter />
        <attrlist>
            <attr floatlength="0" name="OCCASION_ID" maxlength="20" datatype="String" type="PK" remark="发送时机标识">
                <mapingcol datatype="VARCHAR2">OCCASION_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="OCCASION_TYPE_ID" maxlength="20" datatype="String" type="COL" remark="发送时机类型标识">
                <mapingcol datatype="VARCHAR2">OCCASION_TYPE_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="OCCASION_NAME" maxlength="100" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">OCCASION_NAME</mapingcol>
            </attr>
            <attr floatlength="0" name="START_TIME" maxlength="20" datatype="String" type="COL" remark="生效起始时间 时间格式为HH24，且只能是整数，范围只能是0-24，其值必须小于LimitEtime，系统缺省值为0">
                <mapingcol datatype="VARCHAR2">START_TIME</mapingcol>
            </attr>
            <attr floatlength="0" name="END_TIME" maxlength="20" datatype="String" type="COL" remark="生效结束时间">
                <mapingcol datatype="VARCHAR2">END_TIME</mapingcol>
            </attr>
            <attr floatlength="0" name="OPER" maxlength="20" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">OPER</mapingcol>
            </attr>
            <attr floatlength="0" name="OPERATE_TIME" maxlength="20" datatype="DateTime" type="COL" remark="操作时间">
                <mapingcol datatype="DATE">OPERATE_TIME</mapingcol>
            </attr>
            <attr floatlength="0" name="OPERATOR_CODE" maxlength="100" datatype="String" type="COL" remark="操作人">
                <mapingcol datatype="VARCHAR2">OPERATOR_CODE</mapingcol>
            </attr>
            <attr floatlength="0" name="ORDER_STATE" maxlength="3" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">ORDER_STATE</mapingcol>
            </attr>
        </attrlist>
        <relationlist />
        <oplist />
    </sysbo>
</sysbolist>
