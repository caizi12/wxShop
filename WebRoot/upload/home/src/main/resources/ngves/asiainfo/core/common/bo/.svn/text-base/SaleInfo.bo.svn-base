<?xml version="1.0" encoding="UTF-8"?>
<sysbolist>
    <sysbo datasource="UserDataSource" name="SaleInfo" mainattr="SALE_ID">
        <mapingenty type="table">SALE_INFO</mapingenty>
        <datafilter />
        <attrlist>
            <attr floatlength="0" name="SALE_ID" maxlength="15" datatype="Long" type="PK">
                <mapingcol datatype="NUMBER">SALE_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="SALE_NAME" maxlength="200" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">SALE_NAME</mapingcol>
            </attr>
            <attr floatlength="0" name="SALE_TYPE" maxlength="2" datatype="String" type="COL" remark="邮件类型：01   短信类型：02">
                <mapingcol datatype="VARCHAR2">SALE_TYPE</mapingcol>
            </attr>
            <attr floatlength="0" name="SALE_DISCRIPT" maxlength="400" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">SALE_DISCRIPT</mapingcol>
            </attr>
            <attr floatlength="0" name="SALE_FILE" maxlength="400" datatype="String" type="COL" remark="发送的营销短信或邮件中的内容">
                <mapingcol datatype="VARCHAR2">SALE_FILE</mapingcol>
            </attr>
            <attr floatlength="0" name="SEND_STATUS" maxlength="2" datatype="String" type="COL" remark="未发送：0 0  正在发送：01 ,已发送 02   ">
                <mapingcol datatype="VARCHAR2">SEND_STATUS</mapingcol>
            </attr>
            <attr floatlength="0" name="SALE_STATUS" maxlength="2" datatype="String" type="COL" remark="停用：0   启用：1
当执行“删除”操作时，状态置为停用。 且发送状态只能是未发送。">
                <mapingcol datatype="VARCHAR2">SALE_STATUS</mapingcol>
            </attr>
            <attr floatlength="0" name="ADD_TIME" maxlength="20" datatype="DateTime" type="COL">
                <mapingcol datatype="DATE">ADD_TIME</mapingcol>
            </attr>
            <attr floatlength="0" name="SEND_SUM" maxlength="15" datatype="Long" type="COL" remark="每次发送前统计的发送对象总量">
                <mapingcol datatype="NUMBER">SEND_SUM</mapingcol>
            </attr>
            <attr floatlength="0" name="OPEN_SUM" maxlength="15" datatype="Long" type="COL" remark="后续由邮件服务、短信网关提供发送的结果">
                <mapingcol datatype="NUMBER">OPEN_SUM</mapingcol>
            </attr>
            <attr floatlength="0" name="REACH_SUM" maxlength="15" datatype="Long" type="COL" remark="后续由邮件服务、短信网关提供发送的结果">
                <mapingcol datatype="NUMBER">REACH_SUM</mapingcol>
            </attr>
            <attr floatlength="0" name="BAK_COL1" maxlength="15" datatype="Long" type="COL">
                <mapingcol datatype="NUMBER">BAK_COL1</mapingcol>
            </attr>
            <attr floatlength="0" name="BAK_COL2" maxlength="100" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">BAK_COL2</mapingcol>
            </attr>
            <attr floatlength="0" name="BAK_COL3" maxlength="100" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">BAK_COL3</mapingcol>
            </attr>
            <attr floatlength="0" name="FINISH_TIME" maxlength="20" datatype="DateTime" type="COL" remark="发送完成时间">
                <mapingcol datatype="DATE">FINISH_TIME</mapingcol>
            </attr>
        </attrlist>
        <relationlist />
        <oplist />
    </sysbo>
</sysbolist>
