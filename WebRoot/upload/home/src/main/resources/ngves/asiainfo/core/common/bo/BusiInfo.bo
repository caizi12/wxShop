        <datafilter />
        <attrlist>
            <attr floatlength="0" name="BUSI_INFO_ID" maxlength="15" datatype="Long" type="COL">
                <mapingcol datatype="NUMBER">BUSI_INFO_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="BUSI_ID" maxlength="15" datatype="Long" type="COL">
                <mapingcol datatype="NUMBER">BUSI_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="BUSI_TYPE_ID" maxlength="2" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">BUSI_TYPE_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="BUSI_DATE" maxlength="20" datatype="DateTime" type="COL">
                <mapingcol datatype="DATE">BUSI_DATE</mapingcol>
            </attr>
            <attr floatlength="0" name="BUSI_STAFF_ID" maxlength="15" datatype="Long" type="COL">
                <mapingcol datatype="NUMBER">BUSI_STAFF_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="BUSI_COM_TYPE" maxlength="2" datatype="String" type="COL" remark="0-运营中心
1-省公司
2-全网物流商
3-全网供应商
4-仓储商">
