            </attr>
            <attr floatlength="0" name="BUSI_COM_ID" maxlength="15" datatype="Long" type="COL" remark="此处填写省公司代码或合作商编号,总部的代码是：00">
                <mapingcol datatype="NUMBER">BUSI_COM_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="NOTE" maxlength="1024" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">NOTE</mapingcol>
            </attr>
            <attr floatlength="0" name="OPT_TYPE" maxlength="2" datatype="String" type="COL" remark="0-增加/删除/文件下载类 1-修改/审核类 2-查询记录类">
                <mapingcol datatype="VARCHAR2">OPT_TYPE</mapingcol>
            </attr>
            <attr floatlength="0" name="OPT_OBJ" maxlength="1800" datatype="String" type="COL" remark="操作对象">
                <mapingcol datatype="VARCHAR2">OPT_OBJ</mapingcol>
            </attr>
            <attr floatlength="0" name="BAK_COL" maxlength="40" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">BAK_COL</mapingcol>
            </attr>
        </attrlist>
        <relationlist />
        <oplist />
    </sysbo>
</sysbolist>
