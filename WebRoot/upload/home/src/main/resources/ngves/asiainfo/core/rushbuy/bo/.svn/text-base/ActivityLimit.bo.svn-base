<?xml version="1.0" encoding="UTF-8"?>
<sysbolist>
    <sysbo datasource="UserDataSource" name="ActivityLimit" mainattr="ACTIVITY_ID">
        <mapingenty type="table">ACTIVITY_LIMIT</mapingenty>
        <datafilter />
        <attrlist>
            <attr floatlength="0" name="ACTIVITY_ID" maxlength="15" datatype="Long" type="PK" remark="活动标识">
                <mapingcol datatype="NUMBER">ACTIVITY_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="ACTIVITY_NAME" maxlength="150" datatype="String" type="COL" remark="活动名称">
                <mapingcol datatype="VARCHAR2">ACTIVITY_NAME</mapingcol>
            </attr>
            <attr floatlength="0" name="ACTIVITY_START_TIME" maxlength="20" datatype="DateTime" type="COL" remark="活动开始时间">
                <mapingcol datatype="DATE">ACTIVITY_START_TIME</mapingcol>
            </attr>
            <attr floatlength="0" name="ACTIVITY_END_TIME" maxlength="20" datatype="DateTime" type="COL" remark="活动结束时间">
                <mapingcol datatype="DATE">ACTIVITY_END_TIME</mapingcol>
            </attr>
            <attr floatlength="0" name="ACTIVITY_RULE" maxlength="4000" datatype="String" type="COL" remark="活动规则说明">
                <mapingcol datatype="VARCHAR2">ACTIVITY_RULE</mapingcol>
            </attr>
            <attr floatlength="0" name="ACTIVITY_QUESTION" maxlength="4000" datatype="String" type="COL" remark="活动常见问题">
                <mapingcol datatype="VARCHAR2">ACTIVITY_QUESTION</mapingcol>
            </attr>
            <attr floatlength="0" name="ACTIVITY_STATE" maxlength="2" datatype="String" type="COL" remark="活动状态">
                <mapingcol datatype="VARCHAR2">ACTIVITY_STATE</mapingcol>
            </attr>
            <attr floatlength="0" name="ACTIVITY_RECORD_TIME" maxlength="20" datatype="DateTime" type="COL" remark="活动录入时间">
                <mapingcol datatype="DATE">ACTIVITY_RECORD_TIME</mapingcol>
            </attr>
            <attr floatlength="0" name="ACTIVITY_RECORD_PARTNER" maxlength="40" datatype="String" type="COL" remark="活动发布人员">
                <mapingcol datatype="VARCHAR2">ACTIVITY_RECORD_PARTNER</mapingcol>
            </attr>
            <attr floatlength="0" name="ACTIVITY_PUBLIC_TIME" maxlength="20" datatype="DateTime" type="COL" remark="活动发布时间">
                <mapingcol datatype="DATE">ACTIVITY_PUBLIC_TIME</mapingcol>
            </attr>
            <attr floatlength="0" name="ACTIVITY_ACTION" maxlength="4000" datatype="String" type="COL">
		        <mapingcol datatype="VARCHAR2">ACTIVITY_RULE</mapingcol>
            </attr>
            <attr floatlength="0" name="ACTIVITY_PUBLIC_PARTNER" maxlength="40" datatype="String" type="COL" remark="活动录入人员">
                <mapingcol datatype="VARCHAR2">ACTIVITY_PUBLIC_PARTNER</mapingcol>
            </attr>
            <attr floatlength="0" name="ACTIVITY_URL" maxlength="255" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">ACTIVITY_URL</mapingcol>
            </attr>
            <attr floatlength="0" name="ACTIVITY_RESULT" maxlength="4000" datatype="String" type="COL">
		 <mapingcol datatype="VARCHAR2">ACTIVITY_RULE</mapingcol>
            </attr>
            <attr floatlength="0" name="WARE_BIND" maxlength="4000" datatype="String" type="COL">
		 <mapingcol datatype="VARCHAR2">ACTIVITY_RULE</mapingcol>
            </attr>
            <attr floatlength="0" name="MAIN_PIC" maxlength="255" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">MAIN_PIC</mapingcol>
            </attr>
            <attr floatlength="0" name="BG_PIC" maxlength="255" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">BG_PIC</mapingcol>
            </attr>
            <attr floatlength="0" name="BG_COLOR" maxlength="10" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">BG_COLOR</mapingcol>
            </attr>
            <attr floatlength="0" name="AL_GROUP_ID" maxlength="15" datatype="Long" type="COL">
                <mapingcol datatype="NUMBER">AL_GROUP_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="IS_VIEW" maxlength="2" datatype="String" type="COL" remark="门户是否显示">
                <mapingcol datatype="CHAR">IS_VIEW</mapingcol>
            </attr>
            <attr floatlength="0" name="APP_VERSION" maxlength="8" datatype="Long" type="COL">
                <mapingcol datatype="NUMBER">APP_VERSION</mapingcol>
            </attr>
            <attr floatlength="0" name="BAK_COL" maxlength="50" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">BAK_COL</mapingcol>
            </attr>
            <attr floatlength="0" name="BAK_COL2" maxlength="50" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">BAK_COL2</mapingcol>
            </attr>
            <attr floatlength="0" name="BAK_COL3" maxlength="50" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">BAK_COL3</mapingcol>
            </attr>
            
            <attr floatlength="0" name="PAY_TYPE" maxlength="10" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">PAY_TYPE</mapingcol>
            </attr>
            <attr floatlength="0" name="PAY_TIME" maxlength="10" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">PAY_TIME</mapingcol>
            </attr>
        </attrlist>
        <relationlist />
        <oplist />
    </sysbo>
</sysbolist>
