<?xml version="1.0" encoding="UTF-8"?>
<sysbolist>
    <sysbo datasource="UserDataSource" name="BusiCustomCommunion" mainattr="BUSI_ID">
        <mapingenty type="table">BUSI_CUSTOM_COMMUNION</mapingenty>
        <datafilter />
        <attrlist>
            <attr floatlength="0" name="BUSI_ID" maxlength="15" datatype="Long" type="PK">
                <mapingcol datatype="NUMBER">BUSI_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="OPT_DATE" maxlength="20" datatype="DateTime" type="COL">
                <mapingcol datatype="DATE">OPT_DATE</mapingcol>
            </attr>
            <attr floatlength="0" name="SEND_USER" maxlength="2" datatype="String" type="COL" remark="01:一级客服;02:积分平台">
                <mapingcol datatype="VARCHAR2">SEND_USER</mapingcol>
            </attr>
            <attr floatlength="0" name="ACCEPT_USER" maxlength="2" datatype="String" type="COL" remark="01:一级客服;02:积分平台">
                <mapingcol datatype="VARCHAR2">ACCEPT_USER</mapingcol>
            </attr>
            <attr floatlength="0" name="ACTION_TYPE" maxlength="2" datatype="String" type="COL" remark="01:收到投诉派发;02:发送投诉回复;03:收到投诉归档;04:发送投诉退回;05:收到中途意见06:发送中途意见;07:收到投诉再处理;08:收到投诉催办">
                <mapingcol datatype="VARCHAR2">ACTION_TYPE</mapingcol>
            </attr>
            <attr floatlength="0" name="ACTION_BUSI_ID" maxlength="15" datatype="Long" type="COL" remark="对应于各个接口接收或发送表中的标识">
                <mapingcol datatype="NUMBER">ACTION_BUSI_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="BUSI_STATUS" maxlength="2" datatype="String" type="COL" remark="0:等待接收附件;1:附件接收完成;2已完结;">
                <mapingcol datatype="VARCHAR2">BUSI_STATUS</mapingcol>
            </attr>
            <attr floatlength="0" name="APP_VERSION" maxlength="8" datatype="Long" type="COL">
                <mapingcol datatype="NUMBER">APP_VERSION</mapingcol>
            </attr>
            <attr floatlength="0" name="BAK_COL" maxlength="50" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">BAK_COL</mapingcol>
            </attr>
            <attr floatlength="0" name="BAK_COL2" maxlength="50" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">BAK_COL2</mapingcol>
            </attr>
            <attr floatlength="0" name="BAK_COL3" maxlength="50" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">BAK_COL3</mapingcol>
            </attr>
            <attr floatlength="0" name="INDICTSEQ" maxlength="50" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">INDICTSEQ</mapingcol>
            </attr>
        </attrlist>
        <relationlist />
        <oplist />
    </sysbo>
</sysbolist>
