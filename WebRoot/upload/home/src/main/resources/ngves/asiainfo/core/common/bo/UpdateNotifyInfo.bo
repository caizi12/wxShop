<?xml version="1.0" encoding="UTF-8"?>
<sysbolist>
    <sysbo datasource="UserDataSource" name="UpdateNotifyInfo" mainattr="NOTIFY_INFO_ID">
        <mapingenty type="table">UPDATE_NOTIFY_INFO</mapingenty>
        <datafilter />
        <attrlist>
            <attr floatlength="0" name="NOTIFY_INFO_ID" maxlength="15" datatype="Long" type="PK">
                <mapingcol datatype="NUMBER">NOTIFY_INFO_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="NOTIFY_TYPE" maxlength="2" datatype="String" type="COL" remark="通知类型：0-礼品 1-公告 2-广告 3-主推礼品 4-品牌推荐">
                <mapingcol datatype="VARCHAR2">NOTIFY_TYPE</mapingcol>
            </attr>
            <attr floatlength="0" name="ITEM_ID" maxlength="15" datatype="Long" type="COL">
                <mapingcol datatype="NUMBER">ITEM_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="ITEM_STATUS" maxlength="2" datatype="String" type="COL" remark="类型变更状态 0 - 新增 1 - 修改">
                <mapingcol datatype="VARCHAR2">ITEM_STATUS</mapingcol>
            </attr>
            <attr floatlength="0" name="STATUS" maxlength="2" datatype="String" type="COL" remark="更新状态 0-未处理 1-已处理 2-处理异常">
                <mapingcol datatype="VARCHAR2">STATUS</mapingcol>
            </attr>
            <attr floatlength="0" name="FAIL_MESSAGE" maxlength="200" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">FAIL_MESSAGE</mapingcol>
            </attr>
            <attr floatlength="0" name="CREATE_TIME" maxlength="20" datatype="DateTime" type="COL">
                <mapingcol datatype="DATE">CREATE_TIME</mapingcol>
            </attr>
            <attr floatlength="0" name="RELATION_ID" maxlength="15" datatype="Long" type="COL">
                <mapingcol datatype="NUMBER">RELATION_ID</mapingcol>
            </attr>
        </attrlist>
        <relationlist />
        <oplist />
    </sysbo>
</sysbolist>
