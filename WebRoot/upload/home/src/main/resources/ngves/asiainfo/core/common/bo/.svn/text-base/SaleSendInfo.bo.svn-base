<?xml version="1.0" encoding="UTF-8"?>
<sysbolist>
    <sysbo datasource="UserDataSource" name="SaleSendInfo" mainattr="SEND_ID">
        <mapingenty type="table">SALE_SEND_INFO</mapingenty>
        <datafilter />
        <attrlist>
            <attr floatlength="0" name="SEND_ID" maxlength="15" datatype="Long" type="PK">
                <mapingcol datatype="NUMBER">SEND_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="SALE_ID" maxlength="15" datatype="Long" type="COL">
                <mapingcol datatype="NUMBER">SALE_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="SEND_TYPE" maxlength="2" datatype="String" type="COL" remark="邮件类型：01   短信类型：02">
                <mapingcol datatype="VARCHAR2">SEND_TYPE</mapingcol>
            </attr>
            <attr floatlength="0" name="SEND_OBJ" maxlength="50" datatype="String" type="COL" remark="如果是邮件，记录邮箱地址；如果是短信，记录手机号。">
                <mapingcol datatype="VARCHAR2">SEND_OBJ</mapingcol>
            </attr>
            <attr floatlength="0" name="IS_SEND" maxlength="2" datatype="String" type="COL" remark="0 未发送 1 发送成功 2 发送失败">
                <mapingcol datatype="VARCHAR2">IS_SEND</mapingcol>
            </attr>
            <attr floatlength="0" name="SEND_TIME" maxlength="20" datatype="DateTime" type="COL">
                <mapingcol datatype="DATE">SEND_TIME</mapingcol>
            </attr>
            <attr floatlength="0" name="CREATE_TIME" maxlength="20" datatype="DateTime" type="COL">
                <mapingcol datatype="DATE">CREATE_TIME</mapingcol>
            </attr>
            <attr floatlength="0" name="OPT_TIME" maxlength="20" datatype="DateTime" type="COL">
                <mapingcol datatype="DATE">OPT_TIME</mapingcol>
            </attr>
            <attr floatlength="0" name="RLT_ID" maxlength="20" datatype="String" type="COL" remark="记录发送、到达的统计数据ID（可能用到）">
                <mapingcol datatype="VARCHAR2">RLT_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="FAIL_REASON" maxlength="200" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">FAIL_REASON</mapingcol>
            </attr>
            <attr floatlength="0" name="BAK_COL1" maxlength="15" datatype="Long" type="COL">
                <mapingcol datatype="NUMBER">BAK_COL1</mapingcol>
            </attr>
            <attr floatlength="0" name="BAK_COL2" maxlength="20" datatype="String" type="COL">
                <mapingcol datatype="VARCHAR2">BAK_COL2</mapingcol>
            </attr>
        </attrlist>
        <relationlist />
        <oplist />
    </sysbo>
</sysbolist>
