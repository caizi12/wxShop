package ngves.asiainfo.core.tuan.model;

/**
 * 封装团购同步给HP的接口信息
 * @author liuyq7@asiainfo-linkage.com
 *
 */
public class TuanInfoValueToHP {

	//固定01(新增)  02(修改)
	private String optType;
	//02+团兑活动ID
	private String actionID;
	//活动名称
	private String actionName;
	//固定00(达到团兑数量下限即开始拆单。)
	private String optOrderType;
	//活动开始时间
	private String actionStartTime;
	//活动截止时间
	private String actionEndTime;
	//最低人数限制
	private int minParticNum;
	//总购买上限
	private int maxParticNum;
	
	//单人购买上限和礼品编码组成的集合 放在ItemInfo中
	//单人购买上限
	private int maxItemNum;
	//礼品编码(礼品code)
	private String itemID;
	
	
	
	public String getOptType() {
		return optType;
	}
	public void setOptType(String optType) {
		this.optType = optType;
	}
	public String getActionID() {
		return actionID;
	}
	public void setActionID(String actionID) {
		this.actionID = actionID;
	}
	public String getActionName() {
		return actionName;
	}
	public void setActionName(String actionName) {
		this.actionName = actionName;
	}
	public String getOptOrderType() {
		return optOrderType;
	}
	public void setOptOrderType(String optOrderType) {
		this.optOrderType = optOrderType;
	}
	public String getActionStartTime() {
		return actionStartTime;
	}
	public void setActionStartTime(String actionStartTime) {
		this.actionStartTime = actionStartTime;
	}
	public String getActionEndTime() {
		return actionEndTime;
	}
	public void setActionEndTime(String actionEndTime) {
		this.actionEndTime = actionEndTime;
	}
	public int getMinParticNum() {
		return minParticNum;
	}
	public void setMinParticNum(int minParticNum) {
		this.minParticNum = minParticNum;
	}
	public int getMaxParticNum() {
		return maxParticNum;
	}
	public void setMaxParticNum(int maxParticNum) {
		this.maxParticNum = maxParticNum;
	}
	public int getMaxItemNum() {
		return maxItemNum;
	}
	public void setMaxItemNum(int maxItemNum) {
		this.maxItemNum = maxItemNum;
	}
	public String getItemID() {
		return itemID;
	}
	public void setItemID(String itemID) {
		this.itemID = itemID;
	}

}
