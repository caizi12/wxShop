package ngves.asiainfo.core.donated.exception;

public class MgtDonatedException extends MgtException {
	/**
	 * 积分捐赠后台异常类
	 */
	private static final long serialVersionUID = 6414671168261047400L;

	public MgtDonatedException(String message, Throwable cause) {
		super(message, cause);
	}

	public MgtDonatedException(String message) {
		super(message);
	}

	public MgtDonatedException(String code, String message) {
		super(code, message);
	}

	public MgtDonatedException(Throwable cause) {
		super(cause);
	}
}
