package ngves.asiainfo.core.award.exception;

public class MgtAwardException extends MgtException {
	/**
	 * 抽奖活动异常信息类
	 * 
	 * @author xj
	 */
	private static final long serialVersionUID = 7883496059567607785L;

	public MgtAwardException(String message, Throwable cause) {
		super(message, cause);
	}

	public MgtAwardException(String message) {
		super(message);
	}

	public MgtAwardException(String code, String message) {
		super(code, message);
	}

	public MgtAwardException(Throwable cause) {
		super(cause);
	}
}
