package ngves.asiainfo.core.sms.exception;

public class MgtSmsException extends Exception{

	/**
	 * 
	 */
	private static final long serialVersionUID = 7893919118798167617L;
	public MgtSmsException(String message, Throwable cause) {
		super(message, cause);
	}

	public MgtSmsException(String message) {
		super(message);
	}
	
	public MgtSmsException(Throwable cause) {
		super(cause);
	}
}
